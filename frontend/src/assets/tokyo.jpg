      <stop offset="0%" style="stop-color:#16a085;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2980b9;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="400" height="300" fill="url(#grad1)" />
  
  <!-- Tokyo Tower Silhouette -->
  <polygon points="200,50 220,250 180,250" fill="none" stroke="#ffffff" stroke-width="3" />
  <line x1="190" y1="100" x2="210" y2="100" stroke="#ffffff" stroke-width="2" />
  <line x1="185" y1="150" x2="215" y2="150" stroke="#ffffff" stroke-width="2" />
  <line x1="180" y1="200" x2="220" y2="200" stroke="#ffffff" stroke-width="2" />
  
  <!-- Mount Fuji Silhouette -->
  <path d="M50,200 Q200,100 350,200" fill="none" stroke="#ffffff" stroke-width="2" />
  <path d="M100,200 Q200,130 300,200" fill="none" stroke="#ffffff" stroke-width="1" />
  
  <!-- City Buildings -->
