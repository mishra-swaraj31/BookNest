  <circle cx="70" cy="170" r="5" fill="#ffffff" opacity="0.8" />
  <circle cx="90" cy="160" r="4" fill="#ffffff" opacity="0.7" />
  <circle cx="110" cy="165" r="6" fill="#ffffff" opacity="0.9" />
  <circle cx="300" cy="170" r="5" fill="#ffffff" opacity="0.8" />
  <circle cx="320" cy="160" r="4" fill="#ffffff" opacity="0.7" />
  <circle cx="340" cy="165" r="6" fill="#ffffff" opacity="0.9" />
  
  <text x="200" y="280" font-family="Arial" font-size="24" font-weight="bold" fill="#ffffff" text-anchor="middle">Tokyo</text>
</svg>